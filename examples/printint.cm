(main)

(char-of-num (n) (local)
  (ret (n + $0))
)

(print-num (n) (local code)
  (code <- char-of-num n)
  (! putchar code)
  (ret)
)

(mod10 (n) (local)
  (ret (n - ((n / 10) * 10))))

(print-int (n) (local last-digit)
  (ifte (n < 10) ((print-num n)) (
    (last-digit <- mod10 n)
    (n = (n / 10))
    (print-int n)
    (print-num last-digit)
  ))
  (ret)
)

(main () (local x)
  (print-int 42)
  (ret))
