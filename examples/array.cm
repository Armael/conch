(main)

(:void print-string (:*u8 s)
  (local :*u8 i :u8 c)
  (i = s)
  (c = (* i))
  (while c (
    (! putchar c)
    (i = (i + 1))
    (c = (* i))
  ))
  (ret))

(:void main () (local :*u8 a :*u8 i)
  (a <- ! malloc #12)
  (i = a)
  (i := $H) (i = (i + 1))
  (i := $e) (i = (i + 1))
  (i := $l) (i = (i + 1))
  (i := $l) (i = (i + 1))
  (i := $o) (i = (i + 1))
  (i := 0)
  (print-string a)
  (! putchar 10)
  (ret))
