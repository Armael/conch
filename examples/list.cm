(main)

(nil () (local) (ret 0))

(cons (x xs) (local cell cdr)
  (cell <- ! malloc 16)
  (cdr = (cell + 8))
  (cell := x)
  (cdr := xs)
  (ret cell))

(rev (l) (local prev next)
  (prev <- nil)
  (while l (
    (next = (* (l + 8)))
    ((l + 8) := prev)
    (prev = l)
    (l = next)
  ))
  (ret prev))

(print-list (xs) (local car cdr)
  (while xs (
    (car = (* xs))
    (cdr = (* (xs + 8)))
    (! putchar car)
    (xs = cdr)
  ))
  (ret))

(main () (local l)
  (l <- nil)
  (l <- cons $H l)
  (l <- cons $e l)
  (l <- cons $l l)
  (l <- cons $l l)
  (l <- cons $o l)
  (l <- rev l)
  (print-list l)
  (! putchar 10)
  (ret))

